{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "align": [
            true,
            "parameters",
            "statements"
        ],
        "eofline": true,
        "max-line-length": [
            true,
            500
        ],
        "forin": false,
        "prefer-for-of": false,
        "member-ordering": false,
        "no-angle-bracket-type-assertion": false,
        "no-bitwise": false,
        "no-any": false,
        "no-console": false,
        "no-empty": false,
        "no-namespace": false,
        "no-reference": false,
        "no-string-literal": false,
        "no-shadowed-variable": false,
        "no-trailing-whitespace": true,
        "only-arrow-functions": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": {
                    "objects": "always",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "allow-leading-underscore",
            "allow-snake-case"
        ],
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "named": "never",
                "asyncArrow": "always"
            }
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "no-unused-expression": [true, "allow-fast-null-checks"]
    },
    "rulesDirectory": []
}