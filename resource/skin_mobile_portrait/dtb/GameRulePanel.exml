                            <ui:RunTimeLabel id="_subpr4" text.zh="押「虎」( 賠彩－1 賠 1 )" text.cn="押「虎」( 赔彩－1 赔 1 )" text.en="Bet on ‘Tiger’ (Payoff: 1 : 1 )" size="36" textColor="0xffffff" textKey="" wordWrap="true" lineSpacing="10" verticalAlign="middle" x="0" y="0" fontFamily="Helvetica" fontFamily.en="Barlow"/> 
