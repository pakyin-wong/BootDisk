<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="skin_desktop.SegmentedControl" minWidth="100" minHeight="50" xmlns:e="http://ns.egret.com/eui"
        xmlns:w="http://ns.egret.com/wing" xmlns:components="components.*" xmlns:ns1="*" states="up,down">
