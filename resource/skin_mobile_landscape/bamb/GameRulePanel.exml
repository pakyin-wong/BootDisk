                            <e:Group id="_drForm" width="2318" height="1515" height.en="1839">
                                <e:Group id="_drFormGroup" width="2318" height="1515" height.en="1839">
                                    <e:Group id="_drRow1" width="2318" height="140">
                                        <ui:RoundRectShape x="0" y="0" cornerTL_TR_BL_BR="12,12,0,0" stroke="0" fillColor="0x112c4c" fillAlpha="0.5" width="2318" height="140"/>
                                        <e:Group id="drRow1Group">
                                            <e:Group id="_drRow1Item1" width.zh="423" width.cn="423" width.en="423" height="140">
                                                <ui:RunTimeLabel fontFamily="Microsoft JhengHei" fontFamily.en="Barlow" id="_lbldrRow1Item1" horizontalCenter="0" verticalCenter="0" text.zh="「閒」\n首 2 張牌之合計點數" text.cn="「闲」\n首 2 张牌之合计点数" text.en="'Player'\nTwo cards total points" size="36" textColor="0x00b8c3" textKey="" wordWrap="true" lineSpacing="10" verticalAlign="middle"  textAlign="center"/> 
                                            </e:Group>
                                            <e:Group id="_drRow1Item2" width.zh="350" width.cn="350" width.en="370" height="140">
                                                <ui:RunTimeLabel fontFamily="Microsoft JhengHei" fontFamily.en="Barlow" id="_lbldrRow1Item2" horizontalCenter="0" verticalCenter="0" text.zh="「閒」" text.cn="「闲」" text.en="'Player'" size="36" textColor="0x00b8c3" textKey="" wordWrap="true" lineSpacing="10" verticalAlign="middle"  textAlign="center"/> 
