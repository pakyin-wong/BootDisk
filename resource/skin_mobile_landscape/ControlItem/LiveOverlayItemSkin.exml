<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="skin_mobile_landscape.LiveOverlayItemSkin" width="1140" height="945" xmlns:e="http://ns.egret.com/eui"
        xmlns:ui="we.ui.*" xmlns:ba="we.ba.*" xmlns:live="we.live.*" xmlns:w="http://ns.egret.com/wing">
    <e:Group id="_quickBetGroup" width="1140" anchorOffsetY="16" top="380">
        <e:Group>
            <ui:RoundRectShape cornerTL_TR_BL_BR="0,0,24,24" stroke="0" strokeColor="0x000000" fillColor="0x202020"
                               fillAlpha="1" strokeIn="0" strokeInColor="0x3A3F48" right="0" left="0" top="0" bottom="0"
                               touchEnabled="false" touchChildren="false" />
            <e:Component id="_tableLayerNode" name="sidebettingtable" width="1140" />
            <e:Component id="_chipLayerNode" name="sidebettingtable" width="1140" />
            <ui:RoundRectShape cornerTL_TR_BL_BR="0,0,24,24" stroke="0" strokeColor="0x000000" fillColor="0x23282e"
                               fillAlpha="0" strokeIn="2" strokeInColor="0x3A3F48" right="0" left="0" top="0" bottom="0"
                               touchEnabled="false" touchChildren="false" />
        </e:Group>
        <e:Group id="_betChipPanel" width="1140" height="270">
            <ui:BaseImageButton id="_closeButton" width="183" height="150" currentState="bet_overlay_close" x="0"
                                anchorOffsetX="-43" skinName="skin_mobile_portrait.ImageButtonSkinMobileOverlay"
                                useColorFilter="true" bottom="0" />
            <ui:BaseImageButton id="_prevButton" width="183" height="150" currentState="bet_overlay_previous" x="269"
                                anchorOffsetX="43" skinName="skin_mobile_portrait.ImageButtonSkinMobileOverlay"
                                useColorFilter="true" bottom="0" />
            <ui:BetChipSetGridSelected id="_betChipSetGridSelected" width="237" horizontalCenter="15.5"
                                       skinName="skin_mobile.BetChipSkin" bottom="18" labelSize="56" height="142.2"
                                       chipScale="1.3" />
            <ui:BaseImageButton id="_confirmButton" width="360" height="150" currentState="bet_overlay_confirm" x="780"
                                anchorOffsetX="43" skinName="skin_mobile_portrait.ImageButtonSkinMobileOverlay"
                                useColorFilter="true" bottom="0" />
        </e:Group>
    </e:Group>
    <e:Group id="_contentContainer" width="1140">
        <e:Rect fillColor="0x202020" right="0" left="0" top="0" bottom="0" ellipseWidth="48" ellipseHeight="48" />
        <ui:CountdownTimer id="_timer" width="90" height="90" x="7" y="7" skinName="skin_mobile.CountdownTimerRoundSkin" />
        <!--<ui:RunTimeLabel id="_label" anchorOffsetY="0" textAlign="left" fontFamily="Barlow" x="110" y="30" size="48"
                             text="" />
        -->
        <e:Group x="110" y="26">
            <ui:RunTimeLabel id="_label_game" textAlign="left" size="48" text="百家樂" />
            <e:Label id="_label_ID" textAlign="left" y="-10" size="60" text="3" fontFamily="Barlow" anchorOffsetY="0"
                     height="62" />
            <e:layout>
                <e:HorizontalLayout verticalAlign="middle" />
            </e:layout>
        </e:Group>
        <ba:MobileLiveListRoadmap id="_roadmapNode" anchorOffsetY="0" horizontalCenter="0" left="2" right="2" top="108"
                                  bottom="2" height="288"  visible="false"/>
			<e:Group id="_shuffleMask" left="0" right="0" bottom="0" top="108">
				<ui:RoundRectShape left="0" right="0" top="0" bottom="0" fillColor="0x1e2126" alpha="0.8" cornerBL="24" cornerBR="24" stroke="0" cornerTL="0" cornerTR="0"/>
				<e:Image width="166" height="144" horizontalCenter="0" y="35" source="icon-shuffle_png"/>
				<ui:RunTimeLabel id="_shuffleLabel" text="洗牌中" horizontalCenter="0" size="52" height="77" y="179" verticalAlign="middle" textAlign="center" textColor="0xcea56f"/>
			</e:Group>
        <ui:InGameMessage id="_message" anchorOffsetX="238.79" anchorOffsetY="42.5"
                          skinName="skin_mobile_portrait.InGameMessageSkin" width="476" height="85" horizontalCenter="0"
                          y="237" infoBg="m_common_gamestate_message_black_png"
                          successBg="m_common_gamestate_message_green_png" errorBg="m_common_gamestate_message_red_png" />
        <e:Group right="30" y="26">
            <ui:RunTimeLabel id="_betLimit" textAlign="left" y="-10" size="48" text="限紅" fontFamily="Barlow"
                             anchorOffsetY="0" height="62" verticalAlign="middle" alpha=".7" />
            <ui:RunTimeLabel id="_toggler" textAlign="left" fontFamily="Barlow" y="24" size="60" x="924" alpha=".7"
                             verticalAlign="middle" />
            <e:Image source="m_common_panel_icon_dropdown_png" x="920" y="52" width="34" height="19" />
            <e:layout>
                <e:HorizontalLayout verticalAlign="middle" gap="10" />
            </e:layout>
        </e:Group>
        <e:Group anchorOffsetX="0" anchorOffsetY="0" bottom="20" left="20" height="80">
            <ui:GoodRoadLabel id="_goodRoadLabel" skinName="skin_mobile_portrait.GoodRoadLabelSkin" x="0" y="0"
                              visible="false" />
            <e:Group id="_alreadyBetSign" height="80" y="0">
                <e:Rect right="0" left="0" top="0" bottom="0" ellipseWidth="80" ellipseHeight="80" fillAlpha="0.76"
                        fillColor="0x42bc60" />
                <e:Label text="已下注" horizontalCenter="0" verticalCenter="0" size="56" left="30" right="30" />
            </e:Group>
            <e:layout>
                <e:HorizontalLayout/>
            </e:layout>
        </e:Group>
        <ui:RoundRectShape id="_strokeShape" cornerTL_TR_BL_BR="24,24,24,24" stroke="2" strokeColor="0x000000"
                           fillColor="0x23282e" fillAlpha="0" strokeIn="2" strokeInColor="0x3A3F48" right="0" left="0"
                           top="0" bottom="0" touchEnabled="false" touchChildren="false" />
    </e:Group>
    <e:Group id="_betChipSetPanel" width="1566" height="469" horizontalCenter="0" verticalCenter="0">
        <e:Image width="100%" source="m_ba_betarea_chipsbg_round_png" scale9Grid="33,33,0,0" height="100%" y="0" />
        <e:Image width="68" height="40" horizontalCenter="-2" source="m_ba_betarea_chipsbg_triangle_png" rotation="180"
                 y="500" />
        <ui:BetChipSetGrid id="_betChipSet" alpha="0" anchorOffsetX="0" horizontalCenter="0"
                           betChipWidth="100" betChipHeight="100" numChipsInRow="6" chipSizeScale="0.9"
                           horizontalGapSize="130" verticalGapSize="110" labelSize="64" top="50" bottom="20"  left="0" right="0"/>
    </e:Group>
</e:Skin>