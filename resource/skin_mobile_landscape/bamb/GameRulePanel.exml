                            <ui:RunTimeLabel fontFamily="Microsoft JhengHei" fontFamily.en="Barlow" id="_subpr8" text.zh="押「和」" text.cn="押「和」" text.en="Bet on ‘Tie’" size="36" textColor="0xffffff" textKey="" wordWrap="true" lineSpacing="10" verticalAlign="middle" x="0" y="0" /> 
